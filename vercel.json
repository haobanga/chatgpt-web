{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "service/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "service/package.json"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
